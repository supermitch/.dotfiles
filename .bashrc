# Bash command aliases
alias ls="ls -a"
alias ll="ls -al"

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ......="cd ../../../../.."

# Git aliases
alias gs="git status"
alias gb="git branch -va --color"
alias gd="git diff "
alias gc="git commit -m "
alias gaa="git add -A; git status"
alias gpom="git push origin master"
alias glog="git log --graph --decorate --all"

# Vim aliases
alias v="mvim "

